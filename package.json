{
  "name": "qdx",
  "description": "Salesforce command line for deployments",
  "version": "1.4.3",
  "author": "Qamar Islam @qislam",
  "bin": {
    "qdx": "./bin/run"
  },
  "bugs": "https://github.com/qislam/qdx/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^2.2.3",
    "cli-ux": "^5.5.1",
    "csvjson": "^5.1.0",
    "debug": "^4.3.1",
    "execa": "^5.0.0",
    "lodash": "^4.17.20",
    "xml-js": "^1.6.11",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.24.1",
    "@oclif/test": "^1.2.7",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/qislam/qdx",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "qdx",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "qislam/qdx",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "dist": "pkg -t node12-macos,node12-linux,node12-win package.json --out-path dist"
  }
}
